

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "uid": {
      "type": "string"
    },
    "labelType": {
      "type": "string"
    },
    "storeNbr": {
      "type": "string"
    },
    "countryCode": {
      "type": "string"
    },
    "labelDate": {
      "type": "string",
      "pattern": "[0-9]+-[0-9]+-[0-9]+T[0-9]+:[0-9]+:[0-9]{2}.[0-9]+Z"
    }
  },
  "required": [
    "uid",
    "labelType",
    "storeNbr",
    "countryCode",
    "labelDate"
  ]
}

